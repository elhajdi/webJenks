<phpdox xmlns="http://xml.phpdox.net/config">
    <project name="Web-test-jenks" source="${basedir}/../src" workdir="${basedir}/api/xml" >
        <collector publiconly="false">
            <include mask="*.php">
            </include>
            <exclude name="**/vendor/*" />
        </collector>
        <generator output="${basedir}/api">
            <build enabled="true" engine="html" output="html">
                <file extension="html">
                </file>
            </build>
        </generator>
    </project>
</phpdox>
