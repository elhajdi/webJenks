<phpdox xmlns="http://xml.phpdox.net/config">
    <project name="Seminaire-Boss" source="${basedir}/../app" workdir="${basedir}/api/xml" >
        <collector publiconly="false">
            <include mask="*.php">
            </include>
        </collector>
        <generator output="${basedir}/api">
            <build enabled="true" engine="html" output="html">
                <file extension="html">
                </file>
            </build>
        </generator>
    </project>
</phpdox>
